<div class="container mt-5">
  <div class="row">
      <% @favorite_posts.reverse.each do |favorite| %>
    <div class="col-lg-3 col-md-4 col-6 mt-2 ">
      <%= link_to post_photo_path(favorite),class:"btn text-dark" do %>
      <%= image_tag favorite.get_image(10,10),class:"img-fluid mt-2" %>
          <%= favorite.address %>
        <% end %>
       </div>
       <% end %>
  </div>
</div>